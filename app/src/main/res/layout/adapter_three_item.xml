<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white">



    <ImageView
        android:id="@+id/ivCheckTitle"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginStart="13dp"
        android:padding="16dp"
        android:src="@mipmap/uncheck"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <TextView
        android:id="@+id/tvTitle1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginStart="40dp"
        android:text="ID"
        android:textColor="#007CFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivCheckTitle"
        app:layout_constraintLeft_toRightOf="@+id/ivCheckTitle"
        app:layout_constraintTop_toTopOf="@+id/ivCheckTitle" />


    <TextView
        android:id="@+id/tvTitle2"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginStart="55dp"
        android:text="流程名称"
        android:textColor="#007CFF"
        android:textSize="16sp"
        app:layout_constraintLeft_toRightOf="@+id/tvTitle1"
        app:layout_constraintTop_toTopOf="@id/tvTitle1" />

    <TextView
        android:id="@+id/tvTitle3"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginStart="65dp"
        android:text="排序"
        android:textColor="#007CFF"
        android:textSize="16sp"
        android:layout_marginEnd="30dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvTitle2" />



    <ImageView
        android:id="@+id/ivCheckOut"
        android:layout_width="0dp"
        android:padding="16dp"
        android:layout_height="wrap_content"
        android:src="@mipmap/uncheck"
        app:layout_constraintRight_toRightOf="@+id/ivCheckTitle"
        app:layout_constraintLeft_toLeftOf="@id/ivCheckTitle"
        app:layout_constraintTop_toBottomOf="@id/tvTitle3" />


    <TextView
        android:id="@+id/tvLabel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintTop_toTopOf="@+id/ivCheckOut"
        app:layout_constraintBottom_toBottomOf="@+id/ivCheckOut"
        app:layout_constraintLeft_toLeftOf="@+id/tvTitle1" />


    <TextView
        android:id="@+id/tvLabel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="流程名称"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintLeft_toLeftOf="@+id/tvTitle2"
        app:layout_constraintTop_toTopOf="@id/tvLabel1" />

    <TextView
        android:id="@+id/tvLabel3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="排序"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintLeft_toLeftOf="@+id/tvTitle3"
        app:layout_constraintRight_toRightOf="@+id/tvTitle3"
        app:layout_constraintTop_toTopOf="@+id/tvLabel2" />




</androidx.constraintlayout.widget.ConstraintLayout>